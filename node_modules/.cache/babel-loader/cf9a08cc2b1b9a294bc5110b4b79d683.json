 new Promise(async (resolve, reject) => {\n\t\t\ttry {\n\t\t\t\tlet res = await axios.post(url, params);\n\t\t\t\tresolve(res);\n\t\t\t} catch (error) {\n\t\t\t\tlet errorMsg = `请求报错路径：${url} \\n 请求错误信息: ${error}`;\n\t\t\t\treject(error);\n\t\t\t}\n\t\t});\n\t}\n}\n"],"mappings":"AAAA,OAAOA,KAAK,MAAM,kBAAkB;AACpC,OAAOC,EAAE,MAAM,aAAa;;AAE5B;AACA;AACA;AACA,eAAe,MAAMC,QAAQ,CAAC;EAE7B;AACD;AACA;AACA;AACA;AACA;EACC,OAAOC,GAAGA,CAACC,GAAG,EAAEC,MAAM,EAAE;IACvB,OAAO,IAAIC,OAAO,CAAC,OAAOC,OAAO,EAAEC,MAAM,KAAK;MAC7C,IAAI;QACH,IAAIC,KAAK,GAAG,MAAMR,EAAE,CAACS,SAAS,CAACL,MAAM,CAAC;QACtC,IAAIM,GAAG,GAAG,IAAI;QACd,IAAI,CAACN,MAAM,EAAE;UACZM,GAAG,GAAG,MAAMX,KAAK,CAACG,GAAG,CAACC,GAAG,CAAC;QAC3B,CAAC,MAAM;UACNO,GAAG,GAAG,MAAMX,KAAK,CAACG,GAAG,CAACC,GAAG,GAAG,GAAG,GAAGK,KAAK,CAAC;QACzC;QACAF,OAAO,CAACI,GAAG,CAAC;MACb,CAAC,CAAC,OAAOC,KAAK,EAAE;QACf,IAAIC,QAAQ,GAAI,WAAUT,GAAI,eAAcQ,KAAM,EAAC;QACnDE,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;QACrBL,MAAM,CAACI,KAAK,CAAC;MACd;IACD,CAAC,CAAC;EACH;;EAEA;AACD;AACA;AACA;AACA;AACA;EACC,OAAOI,IAAIA,CAACZ,GAAG,EAAEC,MAAM,EAAE;IACxB,OAAO,IAAIC,OAAO,CAAC,OAAOC,OAAO,EAAEC,MAAM,KAAK;MAC7C,IAAI;QACH,IAAIG,GAAG,GAAG,MAAMX,KAAK,CAACgB,IAAI,CAACZ,GAAG,EAAEC,MAAM,CAAC;QACvCE,OAAO,CAACI,GAAG,CAAC;MACb,CAAC,CAAC,OAAOC,KAAK,EAAE;QACf,IAAIC,QAAQ,GAAI,UAAST,GAAI,eAAcQ,KAAM,EAAC;QAClDJ,MAAM,CAACI,KAAK,CAAC;MACd;IACD,CAAC,CAAC;EACH;AACD","ignoreList":[]}]}